<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>webservices-soap</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>PersonWebservice</display-name>
    <servlet-name>PersonWebservice</servlet-name>
    <servlet-class>com.webservices.impl.PersonServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersonWebservice</servlet-name>
    <url-pattern>/PersonWebservice</url-pattern>
  </servlet-mapping>
</web-app>